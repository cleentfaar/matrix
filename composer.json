{
    "name": "cleentfaar/matrix",
    "type": "library",
    "description": "Matrix is a highly extensible library for creating and manipulating matrices",
    "keywords": [
        "cleentfaar",
        "matrix",
        "table"
    ],
    "homepage": "https://github.com/cleentfaar/matrix",
    "license": "MIT",
    "authors": [
        {
            "name": "Cas Leentfaar",
            "email": "info@casleentfaar.com",
            "homepage": "https://github.com/cleentfaar",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.5|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.0|^5.0"
    },
    "suggest": {
        "cleentfaar/matrix-renderer-ascii": "For rendering matrices in ASCii",
        "cleentfaar/matrix-exporter-csv": "For exporting matrices to CSV files"
    },
    "autoload": {
        "psr-4": {
            "CL\\Matrix\\": "src/CL/Matrix"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CL\\Matrix\\Test\\": "tests/src/CL/Matrix/Test",
            "CL\\Matrix\\Tests\\": "tests/src/CL/Matrix/Tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
